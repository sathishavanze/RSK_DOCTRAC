$(".single-item").slick({ autoplay: !0, autoplaySpeed: 2e3 }),
    $(".multiple-items").slick({ autoplay: !0, autoplaySpeed: 2e3,infinite: !0, slidesToShow: 3, slidesToScroll: 1 }),
    $(".responsive-slider").slick({
        dots: !1,
        autoplay: !0, 
        autoplaySpeed: 2000,
        infinite: !0,
        speed: 400,
        slidesToShow: 4,
        slidesToScroll: 1,
        responsive: [
            { breakpoint: 1024, settings: { slidesToShow: 4, slidesToScroll: 1, infinite: !0, dots: !1 } },
            { breakpoint: 600, settings: { slidesToShow: 3, slidesToScroll: 1 } },
            { breakpoint: 480, settings: { slidesToShow: 2, slidesToScroll: 1 } },
        ],
    }),
    $(".center-item-slider").slick({
        centerMode: !0,
        centerPadding: "60px",
        slidesToShow: 3,
        responsive: [
            { breakpoint: 768, settings: { arrows: !1, centerMode: !0, centerPadding: "40px", slidesToShow: 3 } },
            { breakpoint: 480, settings: { arrows: !1, centerMode: !0, centerPadding: "40px", slidesToShow: 1 } },
        ],
    }),
    $(".lazy-load-slider").slick({ lazyLoad: "ondemand", slidesToShow: 3, slidesToScroll: 1 }),
    $(".fade-slider").slick({ dots: !0, infinite: !0, speed: 500, fade: !0, cssEase: "linear", autoplay: !0, autoplaySpeed: 2e3 }),
    $(".slider-syncing-for").slick({ slidesToShow: 1, slidesToScroll: 1, arrows: !1, fade: !0, asNavFor: ".slider-syncing-nav" }),
    $(".slider-syncing-nav").slick({ slidesToShow: 5, slidesToScroll: 1, asNavFor: ".slider-syncing-for", dots: !0, centerMode: !0, focusOnSelect: !0 });
